    "bili": "^3.0.11",
    "eslint": "^4.19.0",
    "eslint-config-ktsn": "^1.0.3",
    "eslint-plugin-html": "^4.0.2",
    "glob": "^7.1.2",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "prettier": "1.11.1",
    "rollup-plugin-svelte": "^4.0.0",
    "svelte": "^1.58.0",
    "svelte-loader": "^2.5.1",
